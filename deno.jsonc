{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ www/dev.ts",

    "fmt": "deno fmt",
    "fmt_check": "deno fmt --check",
    "lint": "deno lint",
    "test": "deno test -A"
  },
  "imports": {
    "preact": "https://esm.sh/preact@10.15.1",
    "$std/": "https://deno.land/std@0.202.0/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
